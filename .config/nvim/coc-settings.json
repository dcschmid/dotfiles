{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "eslint.autoFixOnSave": true,
  "eslint.probe": ["javascript", "javascriptreact", "typescript", "typescriptreact", "html", "vue", "markdown"],
  "prettier.disableSuccessMessage": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "stylelintplus.autoFixOnSave": true,
  "stylelintplus.cssInJs": true,
  "cSpell.userWords": [
    "Autoconfiguration"
  ],
  "cSpell.words": [
    "dotfile"
  ],
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      // customize filetypes to your needs
      "filetypes": ["typescript", "typescriptreact", "graphql"]
    }
  }
}
